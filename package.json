{
  "name": "avalanche-yield-orchestrator",
  "version": "1.0.0",
  "description": "AI agent that monitors wallets and reallocates assets across Avalanche-native DeFi protocols",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "ts-node --esm src/index.ts",
    "build": "tsc",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "start": "node dist/index.js",
    "backtest": "ts-node --esm src/scripts/backtest.ts",
    "simulate": "ts-node --esm src/scripts/simulate.ts",
    "monitor": "ts-node --esm src/scripts/monitor.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/henrysammarfo/avalanche-yield-orchestrator.git"
  },
  "keywords": ["avalanche", "defi", "yield", "orchestrator", "ai-agent"],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/henrysammarfo/avalanche-yield-orchestrator/issues"
  },
  "homepage": "https://github.com/henrysammarfo/avalanche-yield-orchestrator#readme",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "ethers": "^6.15.0",
    "hardhat": "^3.0.1",
    "jest": "^30.0.5",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "axios": "^1.11.0",
    "dotenv": "^17.2.1",
    "langchain": "^0.3.31",
    "openai": "^5.15.0",
    "prisma": "^5.0.0"
  }
}
